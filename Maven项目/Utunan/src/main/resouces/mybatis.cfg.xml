    <!-- 类型别名 -->
    <typeAliases>
        <typeAlias type="com.utunan.pojo.Girl" alias="Girl"/>
        <package name="com.utunan.pojo"/>
    </typeAliases>

    <environments default="dev">
         <environment id="dev">
            <transactionManager type="JDBC"/>
            <dataSource type="UNPOOLED">
                <property name="driver" value="${driver}"/>
                <property name="url" value="${url}"/>
                <property name="username" value="${username}"/>
                <property name="password" value="${password}"/>
            </dataSource>
        </environment>
    </environments>

    <mappers>
        <!--<mapper resource="com/utunan/mapper/GirlMapper.xml"></mapper>-->
        <!--<mapper url="file:///X:xxxxx"/>-->
        <mapper class="com.utunan.mapper.GirlMapper"/>
    </mappers>

</configuration>